{
    "name": "smoothphp/cqrs-es-framework-laravel",
    "description": "Laravel adapter for the SmoothPhp CQRS-ES-Framework",
    "keywords": [
        "cqrs",
        "laravel",
        "event sourcing"
    ],
    "homepage": "https://github.com/SmoothPhp/CQRS-ES-Framework-Laravel",
    "license": "MIT",
    "authors": [
        {
            "name": "Simon Bennett",
            "email": "simon@pixelatedcrow.com",
            "homepage": "https://pixelatedcrow.com",
            "role": "Developer"
        },
        {
            "name": "Jordan Crocker",
            "email": "jordan@jcrocker.uk",
            "homepage": "https://jcrocker.uk",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.1.0",
        "smoothphp/cqrs-es-framework": "~6.0.0",
        "laravel/framework": "^5.2|^6.0",
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "8.*",
        "scrutinizer/ocular": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "SmoothPhp\\LaravelAdapter\\": "src"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "4.x-dev"
        },
        "laravel": {
            "providers": [
                "SmoothPhp\\LaravelAdapter\\ServiceProvider"
            ]
        }
    }
}
